{
  "name": "streamyfin-discord-bot",
  "version": "2.3.1",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "dashboard": "node index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "validate": "node -e \"import('./utils/validation.js').then(m => m.logValidationResults(m.validateEnvironment()))\"",
    "health": "node health-check.js",
    "production:check": "node -e \"import('./utils/production.js').then(m => console.log(JSON.stringify(m.validateProductionEnvironment(), null, 2)))\"",
    "security:audit": "npm audit --audit-level=moderate",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "docker:build": "./docker-manage.sh build",
    "docker:start": "./docker-manage.sh start",
    "docker:dev": "./docker-manage.sh dev",
    "docker:stop": "./docker-manage.sh stop",
    "docker:logs": "./docker-manage.sh logs",
    "docker:status": "./docker-manage.sh status"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "axios": "^1.7.9",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "eldr": "github:bkw/eldr",
    "express": "^4.21.2",
    "ollama": "^0.5.15",
    "redis": "^5.1.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.39.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "semantic-release": "^24.2.5"
  }
}
